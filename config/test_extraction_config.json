{
  "hasura_endpoint": "${HASURA_ENDPOINT}",
  "materialized_views": [
    "mv_curriculum_units"
  ],
  "clear_database_before_import": false,
  "test_limit": 10,
  "node_mappings": [
    {
      "label": "Subject",
      "id_field": "subject_id",
      "properties": {
        "id": {
          "source_field": "subject_id",
          "target_type": "string"
        },
        "title": {
          "source_field": "subject_title",
          "target_type": "string"
        },
        "year_group": {
          "source_field": "year_title",
          "target_type": "string"
        }
      }
    },
    {
      "label": "Unit",
      "id_field": "unit_id",
      "properties": {
        "id": {
          "source_field": "unit_id",
          "target_type": "string"
        },
        "title": {
          "source_field": "unit_title",
          "target_type": "string"
        },
        "description": {
          "source_field": "unit_description",
          "target_type": "string"
        },
        "subject": {
          "source_field": "subject_title",
          "target_type": "string"
        },
        "year": {
          "source_field": "year_title",
          "target_type": "string"
        }
      }
    }
  ],
  "relationship_mappings": [
    {
      "type": "HAS_UNIT",
      "start_node_id_field": "subject_id",
      "end_node_id_field": "unit_id",
      "properties": {}
    }
  ]
}