{
  "hasura_endpoint": "${HASURA_ENDPOINT}",
  "materialized_views": [
    "mv_curriculum_units"
  ],
  "clear_database_before_import": false,
  "test_limit": 20,
  "node_mappings": [
    {
      "label": "Year",
      "id_field": "_generated_uuid",
      "properties": {
        "uuid": {
          "source_field": "_generated_uuid",
          "target_type": "string"
        },
        "business_id": {
          "source_field": "year_id",
          "target_type": "string"
        },
        "title": {
          "source_field": "year_title",
          "target_type": "string"
        },
        "year_number": {
          "source_field": "year_number",
          "target_type": "int"
        }
      }
    },
    {
      "label": "Subject",
      "id_field": "_generated_uuid",
      "properties": {
        "uuid": {
          "source_field": "_generated_uuid",
          "target_type": "string"
        },
        "business_id": {
          "source_field": "subject_id",
          "target_type": "string"
        },
        "title": {
          "source_field": "subject_title",
          "target_type": "string"
        }
      }
    },
    {
      "label": "UnitOffering",
      "id_field": "_generated_uuid",
      "deduplication_key": "year_id,subject_id",
      "properties": {
        "uuid": {
          "source_field": "_generated_uuid",
          "target_type": "string"
        },
        "business_id": {
          "source_field": "_computed_offering_id",
          "computation": "concat:year_id,-,subject_id",
          "target_type": "string"
        },
        "display_name": {
          "source_field": "_computed_offering_name",
          "computation": "concat:year_title, ,subject_title",
          "target_type": "string"
        },
        "year_business_id": {
          "source_field": "year_id",
          "target_type": "string"
        },
        "subject_business_id": {
          "source_field": "subject_id",
          "target_type": "string"
        }
      }
    },
    {
      "label": "Unit",
      "id_field": "_generated_uuid",
      "properties": {
        "uuid": {
          "source_field": "_generated_uuid",
          "target_type": "string"
        },
        "business_id": {
          "source_field": "unit_id",
          "target_type": "string"
        },
        "title": {
          "source_field": "unit_title",
          "target_type": "string"
        },
        "description": {
          "source_field": "unit_description",
          "target_type": "string"
        },
        "order": {
          "source_field": "unit_order",
          "target_type": "int"
        }
      }
    }
  ],
  "relationship_mappings": [
    {
      "type": "HAS_YEAR",
      "start_node_id_field": "_uuid_unitoffering",
      "end_node_id_field": "_uuid_year",
      "match_strategy": "business_id",
      "start_business_key": "year_id,subject_id",
      "end_business_key": "year_id",
      "properties": {}
    },
    {
      "type": "HAS_SUBJECT",
      "start_node_id_field": "_uuid_unitoffering",
      "end_node_id_field": "_uuid_subject",
      "match_strategy": "business_id",
      "start_business_key": "year_id,subject_id",
      "end_business_key": "subject_id",
      "properties": {}
    },
    {
      "type": "OFFERS_UNIT",
      "start_node_id_field": "_uuid_unitoffering",
      "end_node_id_field": "_uuid_unit",
      "match_strategy": "business_id",
      "start_business_key": "year_id,subject_id",
      "end_business_key": "unit_id",
      "properties": {
        "order": {
          "source_field": "unit_order",
          "target_type": "int"
        }
      }
    }
  ]
}