{
  "hasura_endpoint": "https://test.hasura.app/v1/graphql",
  "materialized_views": [
    "mv_curriculum_units",
    "mv_curriculum_lessons",
    "mv_learning_objectives"
  ],
  "clear_database_before_import": true,
  "test_limit": 10,
  "node_mappings": [
    {
      "label": "Subject",
      "id_field": "subject_id",
      "properties": {
        "id": {
          "source_field": "subject_id",
          "target_type": "string"
        },
        "title": {
          "source_field": "subject_title",
          "target_type": "string"
        },
        "description": {
          "source_field": "subject_description",
          "target_type": "string"
        }
      }
    },
    {
      "label": "Unit",
      "id_field": "unit_id",
      "properties": {
        "id": {
          "source_field": "unit_id",
          "target_type": "string"
        },
        "title": {
          "source_field": "unit_title",
          "target_type": "string"
        },
        "description": {
          "source_field": "unit_description",
          "target_type": "string"
        },
        "order": {
          "source_field": "unit_order",
          "target_type": "int"
        }
      }
    },
    {
      "label": "Lesson",
      "id_field": "lesson_id",
      "properties": {
        "id": {
          "source_field": "lesson_id",
          "target_type": "string"
        },
        "title": {
          "source_field": "lesson_title",
          "target_type": "string"
        },
        "description": {
          "source_field": "lesson_description",
          "target_type": "string"
        },
        "duration_minutes": {
          "source_field": "duration_minutes",
          "target_type": "int"
        }
      }
    }
  ],
  "relationship_mappings": [
    {
      "type": "BELONGS_TO",
      "start_node_id_field": "unit_id",
      "end_node_id_field": "subject_id",
      "properties": {}
    },
    {
      "type": "CONTAINS",
      "start_node_id_field": "unit_id",
      "end_node_id_field": "lesson_id",
      "properties": {
        "order": {
          "source_field": "lesson_order_in_unit",
          "target_type": "int"
        }
      }
    }
  ]
}