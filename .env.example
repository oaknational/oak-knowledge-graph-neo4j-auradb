# Environment variables for Oak Knowledge Graph Pipeline
# Copy this file to .env and set actual values

# Hasura GraphQL endpoint
HASURA_ENDPOINT=https://your-hasura-instance.hasura.app/v1/graphql

# Oak Authentication (Required)
HASURA_API_KEY=your-128-character-oak-api-key
OAK_AUTH_TYPE=oak-admin

# Optional: Logging configuration
LOG_LEVEL=INFO
LOG_FILE=

# Optional: Neo4j connection details (for AuraDB import)
NEO4J_URI=bolt://localhost:7687
NEO4J_USERNAME=neo4j
NEO4J_PASSWORD=your-password